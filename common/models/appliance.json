{
  "name": "Appliance",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "GroupContent": true,
    "Cascade": {
      "ApplianceStone": "applianceId"
    },
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "json": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "stones": {
      "type": "hasAndBelongsToMany",
      "model": "Stone",
      "foreignKey": "applianceId"
    },
    "location": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "locationId"
    }
  },
  "acls": [],
  "methods": {}
}
