{
  "name": "Sphere",
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Cascade": {
      "SphereAccess": "sphereId",
      "Location": "sphereId"
    },
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    }
  },
  "hidden": [
    "adminEncryptionKey",
    "memberEncryptionKey",
    "guestEncryptionKey"
  ],
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "uuid": {
      "type": "string",
      "required": true
    },
    "meshAccessAddress": {
      "type": "string"
    },
    "adminEncryptionKey": {
      "type": "string"
    },
    "memberEncryptionKey": {
      "type": "string"
    },
    "guestEncryptionKey": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "ownedLocations": {
      "type": "hasMany",
      "model": "Location",
      "foreignKey": "sphereId"
    },
    "ownedStones": {
      "type": "hasMany",
      "model": "Stone",
      "foreignKey": "sphereId"
    },
    "ownedAppliances": {
      "type": "hasMany",
      "model": "Appliance",
      "foreignKey": "sphereId"
    },
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "sphereId",
      "through": "SphereAccess",
      "keyThrough": ""
    }
  },
  "acls": [],
  "methods": {}
}
