{
  "name": "EnergyUsage",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableCommon": true,
    "SphereContent": true,
    "Util": true
  },
  "properties": {
    "timestamp": {
      "type": "date",
      "default": "$now"
    },
    "energy": {
      "type": "number",
      "required": true
    },
    "duration": {
      "type": "number",
      "required": true
    }
  },
  "hidden": [
    "id",
    "stoneId",
    "sphereId"
  ],
  "validations": [],
  "relations": {
    "appliance": {
      "type": "belongsTo",
      "model": "Appliance",
      "foreignKey": "applianceId"
    },
    "stone": {
      "type": "belongsTo",
      "model": "Stone",
      "foreignKey": "stoneId"
    }
  },
  "acls": [],
  "methods": {}
}
