{
  "name": "TimedLocation",
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "timestamp": {
      "type": "date",
      "default": "$now"
    },
    "locationId": {
      "type": "any",
      "required": true
    },
    "deviceId": {
      "type": "any",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "location": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "locationId"
    },
    "device": {
      "type": "belongsTo",
      "model": "Device",
      "foreignKey": "deviceId"
    }
  },
  "acls": [],
  "methods": {}
}
