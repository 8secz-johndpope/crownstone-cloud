{
  "name": "Hook",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "mixins": {
    "DisableCommon": true,
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    }
  },
  "hidden": [
    "ownerId",
    "sphereId",
    "locationId",
    "stoneId",
    "deviceId",
    "applianceId"
  ],
  "properties": {
    "uri": {
      "type": "string",
      "required": true
    },
    "secret": {
      "type": "string",
      "required": true
    },
    "events": {
      "type": "array",
      "required": true
    },
    "enabled": {
      "type": "boolean",
      "required": true,
      "default":true
    }
  },
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [],
  "methods": {}
}
