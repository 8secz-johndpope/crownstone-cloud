{
  "name": "Firmware",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "mixins": {
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "version": {
      "type": "string",
      "required": true
    },
    "supportedHardwareVersions": {
      "type": "array",
      "required": true
    },
    "minimumCompatibleVersion": {
      "type": "string",
      "required": true
    },
    "downloadUrl": {
      "type": "string",
      "required": true
    },
    "sha1hash": {
      "type": "string",
      "required": true
    }
  },
  "acls": [],
  "methods": {}
}
