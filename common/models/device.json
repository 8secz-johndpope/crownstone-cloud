{
  "name": "Device",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableCommon": true,
    "EnableHooks": true,
    "ChangeTracker": true,
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "OS": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "locale": {
      "type": "string"
    },
    "deviceType": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "hubFunction": {
      "type": "boolean",
      "default": false
    },
    "tapToToggleCalibration": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "currentLocation": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "currentLocationId"
    },
    "currentSphere": {
      "type": "belongsTo",
      "model": "Sphere",
      "foreignKey": "currentSphereId"
    },
    "installations": {
      "type": "hasMany",
      "model": "AppInstallation",
      "foreignKey": "deviceId"
    }
  },
  "acls": [],
  "methods": {}
}
