{
  "name": "Device",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "tapToToggleCalibration": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "currentLocation": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "currentLocationId"
    },
    "scans": {
      "type": "hasMany",
      "model": "Scan",
      "foreignKey": ""
    },
    "locationsHistory": {
      "type": "hasMany",
      "model": "TimedLocation",
      "foreignKey": "deviceId"
    },
    "currentCoordinate": {
      "type": "belongsTo",
      "model": "Coordinate",
      "foreignKey": "currentCoordinateId"
    },
    "coordinatesHistory": {
      "type": "hasMany",
      "model": "Coordinate",
      "foreignKey": "deviceId"
    },
    "installation": {
      "type": "hasOne",
      "model": "Installation",
      "foreignKey": "deviceId"
    }
  },
  "acls": [],
  "methods": {}
}
