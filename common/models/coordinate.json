{
  "name": "Coordinate",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "timestamp": {
      "type": "date",
      "default": "$now"
    },
    "x": {
      "type": "number",
      "required": true
    },
    "y": {
      "type": "number",
      "required": true
    },
    "z": {
      "type": "number",
      "required": true
    },
    "reference": {
      "type": "any"
    },
    "groundTruth": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "stone": {
      "type": "belongsTo",
      "model": "Stone",
      "foreignKey": "stoneId"
    },
    "device": {
      "type": "belongsTo",
      "model": "Device",
      "foreignKey": "deviceId"
    }
  },
  "acls": [],
  "methods": {}
}
