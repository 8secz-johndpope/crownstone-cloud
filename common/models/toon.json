{
  "name": "Toon",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strict":"filter"
  },
  "mixins": {
    "DisableAll": true,
    "SphereContent": true,
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "properties": {
    "toonAgreementId": {
      "type": "string",
      "required": true
    },
    "toonAddress": {
      "type": "string",
      "required": false
    },
    "refreshToken": {
      "type": "string",
      "required": true
    },
    "schedule": {
      "type": "string"
    },
    "changedToProgram": {
      "type": "string",
      "default": "NONE"
    },
    "changedProgramTime": {
      "type": "number",
      "default": 0
    }
  },
  "hidden": [
    "refreshToken",
    "sphereId"
  ],
  "validations": [],
  "relations": {
    "sphere": {
      "type": "belongsTo",
      "model": "Sphere",
      "foreignKey": "sphereId"
    }
  },
  "acls": [],
  "methods": {}
}
