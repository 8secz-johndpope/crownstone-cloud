{
  "name": "Person",
  "plural": "People",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "customId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "hasOne",
      "model": "user",
      "foreignKey": ""
    },
    "teamMembers": {
      "type": "hasAndBelongsToMany",
      "model": "Person",
      "foreignKey": ""
    },
    "devices": {
      "type": "hasMany",
      "model": "Device",
      "foreignKey": "ownerId"
    },
    "ownedLocations": {
      "type": "hasMany",
      "model": "Location",
      "foreignKey": "ownerId"
    },
    "currentLocation": {
      "type": "hasMany",
      "model": "Location",
      "foreignKey": "ownerId",
      "through": "Device"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
