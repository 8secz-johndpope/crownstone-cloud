{
  "name": "Location",
  "replaceOnPUT": false,
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableCommon": true,
    "ChangeTracker": false,
    "EnableHooks": true,
    "SphereContent": true,
    "Cascade": {
      "StoneLocation": "locationId"
    },
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    },
    "Util": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "uid": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "imageId": {
      "type": "any"
    },
    "fingerprint": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "messages":{
      "type": "hasMany",
      "model": "Message",
      "foreignKey": "triggerLocationId"
    },
    "stones": {
      "type": "hasAndBelongsToMany",
      "model": "Stone",
      "foreignKey": "locationId",
      "through": "StoneLocation"
    },
    "presentPeople": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "currentLocationId",
      "through": "Device",
      "keyThrough": "ownerId"
    },
    "fingerprints": {
      "type": "hasMany",
      "model": "Fingerprint",
      "foreignKey": "locationId"
    },
    "sphereOverviewPosition": {
      "type": "hasOne",
      "model": "Position",
      "foreignKey": "locationId"
    }
  },
  "acls": [],
  "methods": {}
}
