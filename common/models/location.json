{
  "name": "Location",
  "base": "PersistedModel",
  "strictDelete": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SphereContent": true,
    "Cascade": {
      "StoneLocation": "locationId"
    },
    "TimeStamps": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "groundTruth": {
      "type": "boolean"
    },
    "icon": {
      "type": "string"
    },
    "imageId": {
      "type": "any"
    }
  },
  "validations": [],
  "relations": {
    "stones": {
      "type": "hasAndBelongsToMany",
      "model": "Stone",
      "foreignKey": "locationId"
    },
    "presentPeople": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "currentLocationId",
      "through": "Device",
      "keyThrough": "ownerId"
    }
  },
  "acls": [],
  "methods": {}
}
