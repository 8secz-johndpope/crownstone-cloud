{
  "name": "Group",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Cascade": {
      "GroupAccess": "groupId"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "uuid": {
      "type": "string",
      "required": true
    },
    "PIN": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "ownedLocations": {
      "type": "hasMany",
      "model": "Location",
      "foreignKey": "groupId"
    },
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "groupId",
      "through": "GroupAccess",
      "keyThrough": ""
    }
  },
  "acls": [],
  "methods": {}
}
