{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "acl_enabled": true,
  "remoting": {
    "rest": {
      "normalizeHttpPath": false,
      "xml": false,
      "handleErrors": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "types":{
      "warnOnUnknownType": false
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "acl_enabled": false,
  "sessionKey": {
    "key": "HIDDEN"
  },
  "ToonIntegration": {
    "clientId": "clientIdHere",
    "clientSecret": "clientSecretHere"
  }
}
