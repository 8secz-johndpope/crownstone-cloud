{
  "initial:before": {
    "loopback#favicon": {},
    "loopback#token": {}
  },
  "initial": {
    "compression": {
      "params": {
        "threshold": 512
      },
      "enabled": true
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {

  },
  "auth": {

  },
  "auth:after": {

  },
  "parse": {},
  "routes:before": {

  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [{
      "params": "$!../client"
    }]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": false,
        "log": true,
        "safeFields": ["errorCode","message"]
      }
    }
  }
}
